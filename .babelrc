{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "chrome": 48,
        "ie": 9,
        "browsers": [
          "> 1%",
          "last 10 versions"
        ]
      }
    }]
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
